{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env" : {
    "es6": true,
    "node": true,
    "browser": true
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
